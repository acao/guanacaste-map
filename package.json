{
  "name": "parcel-sandbox",
  "version": "1.0.0",
  "description": "Simple Parcel Sandbox",
  "main": "index.html",
  "scripts": {
    "start": "rm -rf docs && parcel index.html --open -d docs",
    "build": "rm -rf docs && parcel build index.html -d docs --public-url /guanacaste-map/",
    "precommit": "yarn lint",
    "stylelint": "yarn exec stylelint ./src/styles",
    "eslint": "yarn exec eslint .",
    "lint": "yarn stylelint && yarn eslint"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.5",
    "eslint": "^5.2.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "parcel-bundler": "^1.6.1",
    "prettier": "^1.13.7",
    "prettier-eslint": "^8.8.2"
  }
}
